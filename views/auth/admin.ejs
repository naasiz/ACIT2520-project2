<h1 class="mt-4">Dashboard</h1>
<p class="lead mb-3">Welcome <%= user.name %></p>
<a href="/auth/logout" class="btn btn-secondary">Logout</a>
<p class="mt-3">Current Active Sessions:</p>
<% for(let key in sessions) { %>
<div class="my-3">
    <p class="m-0">SessionId: <%= key %></p>
    <p class="m-0">UserId: <%= sessions[key]['passport']['user'] %></p>
    <a href="/auth/kill-session?sessionId=<%= key %>">Revoke Session</a>
</div>
 <% } %>